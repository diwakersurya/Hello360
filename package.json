{
    "name": "Hello360",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-360/scripts/packager.js",
        "build": "npm run clean && npm-run-all --parallel build:*",
        "build:copy": "cpx './static_assets/*.**' './build/static_assets/'",
        "build:bundle": "node node_modules/react-360/scripts/bundle.js",
        "open": "node -e \"require('xopen')('http://localhost:8081/index.html')\"",
        "devtools": "react-devtools",
        "clean": "rm -rf build",
        "test": "jest"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "cpx": "^1.5.0",
        "i": "^0.3.6",
        "npm": "^6.9.2",
        "npm-run-all": "^4.1.5",
        "react": "16.3.2",
        "react-360": "~1.1.0",
        "react-360-keyboard": "^1.0.2",
        "react-360-web": "~1.1.0",
        "react-native": "~0.55.4",
        "three": "^0.125.0"
    },
    "devDependencies": {
        "babel-jest": "^19.0.0",
        "babel-preset-react-native": "^1.9.1",
        "jest": "^19.0.2",
        "react-devtools": "^2.5.2",
        "react-test-renderer": "16.0.0",
        "xopen": "1.0.0"
    },
    "jest": {
        "preset": "react-360"
    }
}
